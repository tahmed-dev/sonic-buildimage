$(DBSYNCD_PY3)_CACHE_MODE  := GIT_CONTENT_SHA 
$(DBSYNCD_PY3)_DEP_FLAGS   := $(SONIC_COMMON_FLAGS_LIST)
$(DBSYNCD_PY3)_DEP_FILES   := $(DEP_FILES)
$(DBSYNCD_PY3)_SMDEP_FILES := $(SMDEP_FILES)
$(DBSYNCD_PY3)_SMDEP_PATHS := $(SPATH)

